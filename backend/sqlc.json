{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "queries": "./db/query/*.sql",
      "schema": "./db/migrations",
      "gen": {
        "go": {
          "package": "repository",
          "out": "repository",
          "sql_package": "pgx/v5"
        }
      }
    }
  ]
}
